<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="comidas">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="comida" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="nome" type="xs:string"/>
              <xs:element name="calorias" type="xs:integer"/>
              <xs:element name="preco" type="xs:decimal"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>